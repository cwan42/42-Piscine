ex00  ft_strcpy.c

char  *ft_strcpy(char *dest, char *src)
{
    int  i;
    while (src[i] !='\0')
    {
        dest[i] = src[i];
        i++;
    }
    dest[i] = '\0';
    return (dest);
}
