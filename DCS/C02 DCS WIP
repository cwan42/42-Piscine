    ft_strcpy(dest, src);
    printf("%s, %s\n", src, dest);
    return (0);
}

ex01  ft_strncpy.c

#include <stddef.h>

char  *ft_strncpy(char *dest, char *src, unsigned int n)
{
      size_t i;
      i = 0;

      while (i < n && src[i] != '\0')
      {
          dest[i] = src[i];
          i++;
      }
      while (i < n)
      {
          dest[i] = '\0';
          i++;
      }
      return (dest);
}
/*
#include <stdio.h>
int  main(void)
{
    char src[] = "BeFoRee";
    char dest[] = "after";
    size_t n = sizeof(dest) - 1;

    printf("%s -- %s ------", src, dest);
    ft_strncpy(dest, src, n);
    printf("%s, %s\n", src, dest);
    return (0);
}
*/

